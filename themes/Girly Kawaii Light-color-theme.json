        "sideBarSectionHeader.background": "#f5515fbe",//
        "sideBarSectionHeader.foreground": "#360a15",
        "sideBar.border": "#21023f81",
        "sideBar.foreground": "#21023f",
		"statusBar.background": "#6e0053",
		"titleBar.activeBackground": "#6e0053",//barra superior menu file, edit..
