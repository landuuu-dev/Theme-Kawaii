		"activityBarBadge.background": "#e921c4",//icono inferior
		"sideBarTitle.foreground": "#31003d",
		"menu.selectionBackground": "#e95c5c",
        "menu.background": "#e78585",//menus desplegables
        "menu.foreground": "#31003d",
		"menubar.selectionBackground": "#9c049c",// color al pasar el mouse por menu superior
		"activityBar.activeBackground": "#f5515fbe",
		"activityBar.background": "#ff6d92",// menu lateral explorer, busqueda, extensiones...
        "activityBar.border": "#21023f70",
        "activityBar.foreground": "#15002b",
		"banner.background": "#ff0000",
		"commandCenter.background": "#f8a4a4",//barra de busqueda 
        "commandCenter.foreground": "#45174b",//texto barra de busqueda
